<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.MeshView?>

<StackPane prefHeight="300.0" prefWidth="238.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Mesh3DController">
   <children>
      <MeshView fx:id="mesh">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></MeshView>
      <MeshView fx:id="shadow">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></MeshView>
      <Circle fx:id="moveNode" fill="#4d4d4dcc" radius="10.0" stroke="#000000bb" strokeType="INSIDE" translateY="-125.0">
         <cursor>
            <Cursor fx:constant="MOVE" />
         </cursor></Circle>
   </children>
</StackPane>
